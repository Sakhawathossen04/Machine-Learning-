   "outputs": [
    {
     "data": {
      "text/plain": [
       "4321"
      ]
     },
     "execution_count": 17,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "num = 1234\n",
    "rev =0\n",
    "while num>0:\n",
    "    last_digits = int(num%10)\n",
    "    rev  = rev*10 + last_digits\n",
    "    num//=10\n",
    "rev"
   ]
  }
