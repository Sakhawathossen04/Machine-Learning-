   "source": [
    "# Check palindrome number"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "439ff10c-cf68-496a-9c30-73979b21f25f",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Enter the value 121\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Palindrome\n"
     ]
    }
   ],
   "source": [
    "num = int(input(\"Enter the value\"))\n",
    "orginal_num=num\n",
    "rev =0\n",
    "while num>0:\n",
    "    last_digits = int(num%10)\n",
    "    rev  = rev*10 + last_digits\n",
    "    num//=10\n",
    "\n",
    "if orginal_num == rev:\n",
    "    print(\"Palindrome\")\n",
    "else:\n",
    "    print(\"Not Palindrome\")"
   ]
